{
    "type": "object",
    "properties": {
        "gridSpatialRepresentation":{
            "type":"array",
            "title": "Raster spatial representation",
            "items":{
                "type": "object",
                "description":"GridSpatialRepresentation",
                "required": [
                    "transformationParameterAvailability"
                ],
                "properties":{
                    "numberOfDimensions":{
                        "type": "string"
                    },
                    "transformationParameterAvailability":{
                        "type": "boolean"
                    },
                    "cellGeometry":{
                        "type": "string",
                        "enum":[
                            "area",
                            "point"
                        ]
                    },
                    "dimension":{
                        "type": "array",
                        "items": {
                            "type":"object",
                            "properties":{
                                "dimensionName":{
                                    "type":"string",
                                    "enum":[
                                        "row",
                                        "column",
                                        "vertical",
                                        "track",
                                        "crossTrack",
                                        "line",
                                        "sample",
                                        "time"
                                    ]
                                },
                                "dimensionSize":{
                                    "type":"string"
                                },
                                "resolution": {
                                    "type":"object",
                                    "properties":{
                                        "uom": {
                                            "type":"string"
                                        },
                                        "value":{
                                            "type":"string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "vectorSpatialRepresentation":{
            "type":"array",
            "title": "Vector spatial representation",
            "items":{
                "type": "object",
                "description":"VectorSpatialRepresentation",
                "properties":{
                    "topologyLevel":{
                        "type":"string",
                        "enum": [
                            "geometryOnly",
                            "topology1D",
                            "fullPlanarGraph",
                            "surfaceGraph",
                            "fullSurfaceGraph",
                            "topology3D",
                            "fullTopology3D",
                            "abstract"
                        ],
                        "description":"MD_TopologyLevelCode"
                    },
                    "geometricObjects":{
                        "type": "array",
                        "items": {
                            "type":"object",
                            "properties":{
                                "geometricObjectCount":{
                                    "type":"string"
                                },
                                "geometricObjectType":{
                                    "type":"string",
                                    "enum":[
                                        "surface",
                                        "complex",
                                        "composite",
                                        "curve",
                                        "point",
                                        "solid"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        }
    }
